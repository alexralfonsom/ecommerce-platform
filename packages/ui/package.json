{
  "name": "@repo/ui",
  "version": "0.0.0",
  "private": true,
  "files": [
    "dist"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./styles.css": "./dist/styles.css",
    "./configs/DesignSystem": {
      "types": "./dist/configs/DesignSystem.d.ts",
      "import": "./dist/configs/DesignSystem.js",
      "require": "./dist/configs/DesignSystem.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build:styles": "tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "build:components": "tsc && node scripts/post-build.js && tsc-alias",
    "build": "pnpm build:styles && pnpm build:components",
    "type-check": "tsc --noEmit",
    "dev:styles": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "dev:components": "tsc --watch",
    "lint": "eslint src --max-warnings 0",
    "clean": "rm -rf dist"
  },

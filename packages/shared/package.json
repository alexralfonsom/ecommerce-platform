{
  "name": "@repo/shared",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./configs/*": {
      "types": "./dist/configs/*.d.ts",
      "import": "./dist/configs/*.js"
    },
    "./data": {
      "types": "./dist/data/index.d.ts",
      "import": "./dist/data/index.js"
    },
    "./data/mocks": {
      "types": "./dist/data/mocks/index.d.ts",
      "import": "./dist/data/mocks/index.js"
    },
    "./data/mocks/*": {
      "types": "./dist/data/mocks/*.d.ts",
      "import": "./dist/data/mocks/*.js"
    },
    "./lib/api": {
      "types": "./dist/lib/api/index.d.ts",
      "import": "./dist/lib/api/index.js"
    },
    "./lib/utils": {
      "types": "./dist/lib/utils/index.d.ts",
      "import": "./dist/lib/utils/index.js"
    },
    "./lib/hooks": {
      "types": "./dist/lib/hooks/index.d.ts",
      "import": "./dist/lib/hooks/index.js"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts",
      "import": "./dist/types/*.js"
    }
  },
  "dependencies": {
    "@formatjs/intl-localematcher": "^0.6.1",
    "@tanstack/react-query": "^5.90.2",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@types/negotiator": "^0.6.4",
    "@types/node": "^24.5.2",
    "clsx": "^2.1.1",
    "eslint-config-next": "^15.5.4",
    "jspdf": "^3.0.3",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.544.0",
    "negotiator": "^1.0.0",
    "next": "^15.5.4",
    "next-auth": "^4.24.11",
    "next-intl": "^4.3.9",
    "tailwind-merge": "^3.3.1",
    "xlsx": "^0.18.5",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "tsc-alias": "^1.8.16",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "dev": "tsc --watch",
    "clean": "rm -rf dist tsconfig.tsbuildinfo",
    "type-check": "tsc --noEmit"
  }
}
